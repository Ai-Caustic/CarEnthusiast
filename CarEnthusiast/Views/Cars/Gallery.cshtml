                    @if (Model != null)
                    {
                        @foreach (var item in Model)
                        {
                            @if (item.CarImages != null && item.CarImages.Count > 0)
                            {
                                    @for (int k = 0; k < item.CarImages.Count; k++)
                                    {
                                    var imageData = item.CarImages[k].ImageData;
                                    var imageBase64 = Convert.ToBase64String(imageData);
                                    var imageUrl = $"data:image/jpg;base64,{imageBase64}";
                                    <div class="col-sm-6 col-md-4 col-lg-3 col-xl-3 item" data-aos="fade" data-src="@imageUrl" data-sub-html="<h2>@item.Make</h2><h3>@item.Model</h3><h4>@item.Showroom</h4><p>@item.Year</p>">
                                        <a href="#"><img src="@imageUrl" alt="@item.Model" class="img-fluid custom-img"></a>
                                    </div>
                                    }
                            }
                            else
                            {
                                <p>No images available</p>
                            }
                        }
                        }
